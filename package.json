{
    "name": "react-express-webpack-babel-setup",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "client-dist": "NODE_ENV=production webpack --mode production --progress --colors",
        "client-dev": "webpack-dev-server --mode development --colors --hot --inline --host 0.0.0.0 --port 8080",
        "srv-dist": "DEBUG=tetris:* babel src --out-dir dist",
        "srv-dev": "DEBUG=tetris:* nodemon --experimental-worker --exec babel-node ./src/server/index.js",
        "dev": "concurrently --kill-others \"npm run client-dev\" \"npm run srv-dev\"",
        "lint": "eslint src",
        "lint:fix": "eslint --fix src",
        "format": "prettier './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "test": "jest test/**/**/*.test.js --verbose --coverage",
        "test:watch": "npm test -- --watch-All"
    },
    "keywords": [],
    "author": "",
    "license": "",
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "clsx": "^1.1.1",
        "connected-react-router": "^6.8.0",
        "express": "^4.16.4",
        "history": "^4.10.1",
        "notistack": "^1.0.0",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-animations": "^1.0.0",
        "react-countdown": "^2.3.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.1",
        "react-router-dom": "^5.2.0",
        "react-virtualized": "^9.22.2",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "socket.io-middleware": "^2.0.1",
        "styled-components": "^5.2.0",
        "uuid": "^8.3.0",
        "winston": "^2.4.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "7.2.2",
        "@babel/node": "^7.10.5",
        "@babel/preset-env": "7.2.3",
        "@babel/preset-react": "7.0.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^24.9.0",
        "babel-loader": "8.0.4",
        "concurrently": "^5.3.0",
        "css-loader": "^3.6.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-react-hooks": "^1.7.0",
        "file-loader": "^4.3.0",
        "image-webpack-loader": "^6.0.0",
        "jest": "^24.9.0",
        "nodemon": "^1.18.10",
        "prettier": "^2.2.1",
        "react-test-renderer": "^16.13.1",
        "regenerator-runtime": "^0.13.7",
        "style-loader": "^1.2.1",
        "superagent": "^5.3.1",
        "supertest": "^4.0.2",
        "testdouble": "^3.16.1",
        "webpack": "4.28.3",
        "webpack-cli": "3.1.2",
        "webpack-dev-server": "3.1.14"
    },
    "jest": {
        "transform": {
            "\\.(js|jsx)$": "babel-jest",
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
        }
    }
}
