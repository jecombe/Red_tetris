{
  "name": "react-express-webpack-babel-setup",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "client-dist": "NODE_ENV=production webpack --mode production --progress --colors",
    "client-dev": "webpack-dev-server --mode development --colors --hot --inline --host 0.0.0.0 --port 8080",
    "srv-dist": "DEBUG=tetris:* babel src --out-dir dist",
    "srv-dev": "DEBUG=tetris:* nodemon --experimental-worker --exec babel-node ./src/server/server.js",
    "dev": "concurrently --kill-others \"npm run client-dev\" \"npm run srv-dev\"",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "jest test/react.test.js --verbose --coverage",
    "test:watch": "npm test -- --watch-All"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "dependencies": {
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "express": "^4.16.4",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-animations": "^1.0.0",
    "react-dom": "^16.8.1",
    "react-loadingg": "^1.7.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-router-prop-types": "^1.0.4",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-socket.io": "^1.4.0",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "socket.io-middleware": "^2.0.1",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "7.2.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.4",
    "concurrently": "^5.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
